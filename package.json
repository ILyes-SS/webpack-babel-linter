{
  "name": "webpack-babel-linter",
  "version": "1.0.0",
  "description": "a template",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --open",
