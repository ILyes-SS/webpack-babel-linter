    "@babel/preset-env": "^7.24.7",
    "@eslint/js": "^9.5.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.6.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.2",
    "style-loader": "^4.0.0",
